    <!-- Page info -->
    <div class="page-top-info">
        <div class="container">
            <h4>محصولات </h4>
            <div class="site-pagination">
                <a href="">صفحه اصلی</a> /
                <a href="">خرید</a>
            </div>
        </div>
    </div>
    <!-- Page info end -->
    <!-- product section -->
    <section class="product-section">
        <div class="container">
            <div class="back-link">
                <a href="#"> &lt;&lt; بازگشت به لیست خرید </a>
            </div>
            <div class="row">

                <div class="col-lg-6">
                    <div class="product-pic-zoom">
                        <img class="product-big-img" src="~/MyPictures/@Model.ImageProduct" alt="">
                    </div>
                    <div class="product-thumbs" tabindex="1" style="overflow: hidden; outline: none;">
                        <div class="product-thumbs-track">
                            <div class="pt active" data-imgbigurl="~/MyPictures/@Model.ImageProduct">
                                <img src="~/MyPictures/@Model.ImageProduct" alt="">
                            </div>
                            <div class="pt" data-imgbigurl="img/product/p21.jpg">
                                <img src="~/MyPictures/@Model.ImageProduct" alt="">
                            </div>
                            <div class="pt" data-imgbigurl="img/product/p21.jpg">
                                <img src="~/MyPictures/@Model.ImageProduct" alt="">
                            </div>
                            <div class="pt" data-imgbigurl="img/product/p21.jpg">
                                <img src="~/MyPictures/@Model.ImageProduct" alt="">
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-lg-6 product-details text-right">
                    <h2 class="p-title">
                        @Model.Name
                    </h2>
                    <input type="text" value="@Model.Price" style="display:none;" id="PriceProduct" />
                    <input type="text" value="@Model.Name" style="display:none;" id="NameProduct"/>
                    <input type="text" value="@Model.ImageProduct" style="display:none;" id="ImagePath" />
                    <input type="text" value="@Model.IdProduct" style="display:none;" id="IdProduct" />
                    <h3 class="p-price">@Model.Price تومان</h3>
                    <h4 class="p-stock">امتیاز </h4>
                    <div class="p-rating">
                        <i class="fas fa-star"></i>
                        <i class="fas fa-star"></i>
                        <i class="fas fa-star"></i>
                        <i class="fas fa-star"></i>
                        <i class="fas fa-star fa-fade"></i>
                    </div>
                    <div class="p-review">
                        <a href="">4</a><a href="">تا فروخته شده </a>
                    </div>
                    <div class="fw-size-choose">
                        <p>اندازه</p>
                        <div class="sc-item">
                            <input type="radio" name="sc" id="xs-size">
                            <label for="xs-size">12</label>
                        </div>
                        <div class="sc-item">
                            <input type="radio" name="sc" id="s-size">
                            <label for="s-size">14</label>
                        </div>
                        <div class="sc-item">
                            <input type="radio" name="sc" id="m-size" checked="">
                            <label for="m-size">15.6</label>
                        </div>
                        <div class="sc-item">
                            <input type="radio" name="sc" id="l-size">
                            <label for="l-size">17</label>
                        </div>
                        <div class="sc-item disable">
                            <input type="radio" name="sc" id="xl-size" disabled>
                            <label for="xl-size">19</label>
                        </div>
                        <div class="sc-item">
                            <input type="radio" name="sc" id="xxl-size">
                            <label for="xxl-size">22</label>
                        </div>
                    </div>
                    <div class="quantity">
                        <p class="pl-5">تعداد</p>
                        <div class="pro-qty"><input type="text" value="1" id="NumbersOfProduct"></div>
                    </div>
                    <a href="#" class="site-btn" onclick="AddToCart()">افزودن به سبد خرید</a>
                    <div id="accordion" class="accordion-area">
